{
  "name": "ssh2shell",
  "version": "1.4.2",
  "author": "cmp-202",
  "description": "A wrapper class for node.js ssh2 to run multiple sequential commands in an SSH shell session, handle command responses, and tunnel through to other hosts using nested host objects",
  "main": "./lib/ssh2shell.js",
  "engines": {
    "node": ">=0.8.7"
  },
  "dependencies": {
    "ssh2": ">=0.3.6"
  },
  "devDependencies": {
    "coffee-script": ">=1.8.0",
    "dotenv": ">=0.4.0"
  },
  "scripts": {
    "prepublish": "coffee -o lib/ src/"
  },
  "keywords": [
    "ssh",
    "ssh2",
    "secure",
    "shell",
    "server",
    "multiple",
    "commands",
    "tunnel",
    "tunnelling",
    "deploy",
    "host",
    "automation",
    "nested",
    "hosts"
  ],
  "repository": {
    "type": "git",
    "url": "http://github.com/cmp-202/ssh2shell"
  },
  "bugs": {
    "url": "https://github.com/cmp-202/ssh2shell/issues"
  },
  "homepage": "https://github.com/cmp-202/ssh2shell/wiki",
  "directories": {
    "test": "test"
  },
